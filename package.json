{
  "name": "ofkm-shadcn-registry",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "cloudflare": "pnpm build && pnpm registry:build",
    "registry:build": "shadcn-svelte registry build",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.37.0",
    "@internationalized/date": "^3.10.0",
    "@lucide/svelte": "^0.545.0",
    "@shikijs/langs": "^3.13.0",
    "@shikijs/themes": "^3.13.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.46.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.14",
    "bits-ui": "^2.11.5",
    "clsx": "^2.1.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.4",
    "globals": "^16.4.0",
    "isomorphic-dompurify": "^2.28.0",
    "package-manager-detector": "^1.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "runed": "^0.31.1",
    "shiki": "^3.13.0",
    "svelte": "^5.39.11",
    "svelte-check": "^4.3.3",
    "svelte-toolbelt": "^0.9.3",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.14",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.9"
  },
  "dependencies": {
    "@sveltejs/adapter-node": "^5.3.3",
    "mode-watcher": "^1.1.0",
    "shadcn-svelte": "^1.0.8"
  }
}
